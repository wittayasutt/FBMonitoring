{
	"plugins": [
		[
			"module-resolver",
			{
				"root": ["."],
				"alias": {
					"styles": "./styles"
				},
				"cwd": "babelrc"
			}
		],
		[
			"wrap-in-js",
			{
				"extensions": ["css$", "scss$"]
			}
		],
		[
			"styled-components",
			{ "ssr": true, "displayName": true, "preprocess": false }
		]
	],
	"presets": [
		[
			"next/babel",
			{
				"styled-jsx": {
					"plugins": ["styled-jsx-plugin-postcss"]
				}
			}
		]
	],
	"ignore": []
}
